  <data name="css" xml:space="preserve">
    <value>.post_img {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    vertical-align: top
}
a {
    color: #2a5885;
    text-decoration: none;
    cursor: pointer;
}
#date {
	color:#939393;
}
.author{
	font-weight: 500;
    -webkit-font-smoothing: subpixel-antialiased;
    -moz-osx-font-smoothing: auto;
	display: inline-block;
	margin-left: 10px;
	margin-top: 10px;
}
.post_date {
    display: -ms-flexbox;
    display: flex;
    white-space: pre;
    position: relative;
    font-size: 12.5px;
    padding-top: 3px;
}

.button {
	padding: 8px 16px 8px;
    margin: 15px 0px 0 90px;
    font-size: 12.5px;
    display: inline-block;
    zoom: 1;
    cursor: pointer;
    white-space: nowrap;
    outline: none;
    font-family: -apple-system,BlinkMacSystemFont,Roboto,Helvetica Neue,Geneva,Noto Sans Armenian,Noto Sans Bengali,Noto Sans Cherokee,Noto Sans Devanagari,Noto Sans Ethiopic,Noto Sans Georgian,Noto Sans Hebrew,Noto Sans Kannada,Noto Sans Khmer,Noto Sans Lao,Noto Sans Osmanya,Noto Sans Tamil,Noto Sans Telugu,Noto Sans Thai,sans-serif;
    vertical-align: top;
    line-height: 10px;
    text-align: center;
    text-decoration: none;
    background: none;
    background-color: #5181b8;
    color: #fff;
    border: 0;
    border-radius: 4px;
    box-sizing: border-box;
}
.post {
	margin:13px;
	color: #4a4a4a;
}
.info_div {
	
}
#post_text {
	color:#2a5885;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
	width: 400px;
}
#info_line {
    text-align: right;
    width: 100px;
}
#info_data {
	margin-left: 5px;
}
#inPostImg {
	padding:5px;
}

#end_line {
    width: 400px;
    height: 1px;
    background-color: #bfbfbf;
    margin-top: 15px;
	margin-bottom: 15px;
}</value>
