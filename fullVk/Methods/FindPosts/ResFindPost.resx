  <data name="html" xml:space="preserve">
    <value>&lt;a href="{post_url}" target="_blank" class="info_div" &gt;
	&lt;div class="authimg"&gt;
		&lt;img src="{Avatar}" width="50" height="50" class="post_img" data-alt="{Author}"/&gt;
		&lt;div class="author" &gt;{Author}&lt;br&gt;  &lt;div id="date"&gt;Отправлено {DatePost}&lt;/div&gt;&lt;/div&gt;
		
		
	&lt;/div&gt;
	
	&lt;div class="post" &gt;
		&lt;div id="post_text" &gt;{TittlePost}&lt;/div&gt;
		
		&lt;div style="display:flex" &gt;
			&lt;div id="info_line"&gt; Изображений: &lt;/div&gt;
			&lt;div id="info_data"&gt;{imgCount}&lt;/div&gt;
		&lt;/div&gt;
		&lt;div style="display:flex" &gt;
			&lt;div id="info_line"&gt; Видео: &lt;/div&gt;
			&lt;div id="info_data"&gt;{videoCount}&lt;/div&gt;
		&lt;/div&gt;
		&lt;div style="display:flex" &gt;
			&lt;div id="info_line"&gt; Треков: &lt;/div&gt;
			&lt;div id="info_data"&gt;{tracksCount}&lt;/div&gt;
		&lt;/div&gt;
	&lt;/div&gt;
	
&lt;div id="end_line" /&gt;</value>
